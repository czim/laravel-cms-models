{
    "name": "czim/laravel-cms-models",
    "description": "Laravel CMS: Main Models Module Generator",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com"
        }
    ],
    "keywords": [ "cms", "laravel" ],
    "homepage": "https://github.com/czim",
    "require": {
        "php": ">=5.6.4",
        "czim/laravel-filter": "^1.0",
        "czim/laravel-repository": "~1.4",
        "phpdocumentor/reflection-docblock": "^3.1"
    },
    "require-dev": {
        "czim/laravel-cms-core": "dev-laravel-5.4",
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.4",
        "orchestra/database": "~3.4",
        "dimsav/laravel-translatable": "^5.6",
        "czim/laravel-listify": "^1.0",
        "codesleeve/laravel-stapler": "^1.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\CmsModels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\CmsModels\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
