{
    "name": "czim/laravel-cms-models",
    "description": "Laravel CMS: Main Models Module Generator",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com"
        }
    ],
    "keywords": [ "cms", "laravel" ],
    "homepage": "https://github.com/czim",
    "require": {
        "php": ">=7.1.3",
        "czim/laravel-filter": "^1.0",
        "czim/laravel-repository": "~1.4",
	    "doctrine/dbal": "^2.6",
        "phpdocumentor/reflection-docblock": "^3.1|^4.1"
    },
    "require-dev": {
        "czim/laravel-cms-core": "~1.6.0",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "~3.7",
        "orchestra/database": "~3.7",
        "dimsav/laravel-translatable": "^9.0",
        "czim/laravel-listify": "^1.0",
        "czim/laravel-paperclip": "^1.5",
        "satooshi/php-coveralls": "^1.0",
        "symfony/dom-crawler": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Czim\\CmsModels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\CmsModels\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
