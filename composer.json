{
    "name": "czim/laravel-cms-models",
    "description": "Laravel CMS: Main Models Module Generator",
    "keywords": [ "cms", "laravel" ],
    "homepage": "https://github.com/czim",
    "require": {
        "php": ">=5.5.9",
        "czim/laravel-cms-core": "^0.9.1",
        "czim/laravel-repository": "^1.0",
        "phpdocumentor/reflection-docblock": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\CmsModels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\CmsModels\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
